#include <iostream>
#include <stdlib.h>
#include <ctime>
using namespace std;

int telefono[12000][3];
void llamarST(){
    for (int i=0; i<12000; i++){
        telefono [i][0]=((rand()%2+5)*10000000)+(rand()%(9999999-3000000+1)+1000000);
        telefono [i][1] = (rand()%5)+1;
        telefono [i][2] = rand()%10000;
    }
}
void imprimirST(){
    cout<<"TELEFONO:     PLAN:     IDCLIENTE: "<<endl;
    for (int i=0; i<12000; i++){
        cout<<telefono [i][0]<<"       "<<telefono [i][1]<<"       "<<telefono [i][2]<<endl;
    }
}

int main(){
    
    srand(time(0));
    llamarST();
    imprimirST();
    
    return 0;
}

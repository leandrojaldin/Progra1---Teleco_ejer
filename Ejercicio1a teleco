std::string NombreCliente[10000];

void rellenar_NombreCliente()
{
    int k=0;
    for (int i=0;i<100;i++)
    {
        for (int j=0;j<100;j++)
        {
            NombreCliente[k]=
                nombres[i] + 
                " " +
                apellidos[j];
            k++;
        }
    }
}

void imprimir_NombreCliente()
{
    for (int k=0;k<10000;k++)
    {
        std::cout << NombreCliente[k]
                  << std::endl;
    }
}

int main() {
    std::cout << "Ejercicio 1 inciso a "
              << std::endl;
    
    auto inicio = 
        std::chrono::high_resolution_clock::now();
    rellenar_NombreCliente();
    auto medio = 
        std::chrono::high_resolution_clock::now();
    imprimir_NombreCliente();
    auto fin = 
        std::chrono::high_resolution_clock::now();

    auto duracion1 = 
        std::chrono::duration_cast<std::chrono::microseconds>
        (medio - inicio);

    auto duracion2 = 
        std::chrono::duration_cast<std::chrono::microseconds>
        (fin - medio);

    std::cout 
        << "Tiempo de Generacion de Nombres " 
        << duracion1.count() << " microsegundos" 
        << std::endl;
    
    std::cout 
        << "Tiempo de Impresion de Nombres " 
        << duracion2.count() << " microsegundos" 
        << std::endl;

    return 0;
}
